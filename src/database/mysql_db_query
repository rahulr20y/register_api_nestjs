show TABLES;
 
 
-- Table for NESTJS API
 
CREATE TABLE usersnest (
 user_id INT PRIMARY KEY,
 first_name VARCHAR(40),
 last_name VARCHAR(40),
 email VARCHAR(40),
 pass VARCHAR(40)
);
 
